 <div class="container col-md-12" id="gradient">
    <div class="col-md-12">

    <h1>Login</h1>
<%= form_tag '/login', method: :post do %>
  <div>
    <%= label_tag :phone_number %>
    <%= text_field_tag :phone_number %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Submit" %>
<% end %>

</div>
</div>
