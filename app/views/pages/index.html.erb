          {{ anatomy.anatomy_name }}
          <!-- </div> -->
       </div>
       <button ng-click="addAnatomyUsers()">Submit</button>
      </div>
    </div>
    <div class="col-md-3">
     <%= image_tag("optimized_animated.svg") %>
    </div>
    <div class="col-md-12">
        <div style='width: 800px;'>
          <div id="custom_style" style='width: 800px; height: 100px;'></div>
        </div>
    </div>
</div>

